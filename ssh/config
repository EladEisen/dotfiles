	HostName homebrew.abb-edi.fluency.net.uk
	ForwardAgent yes
	User brew

Host homebrew-esx-1
	HostName 10.0.1.3

Host homebrew-esx-2
	HostName 10.0.1.4

Host homebrew-esx-3
	HostName 10.0.1.5

