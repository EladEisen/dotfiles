Host homebrew-server
	HostName homebrew.abb-edi.fluency.net.uk
	ForwardAgent yes
	Port 22122
	User brewadmin

Host homebrew-vm-mojave-1
	Port 22131

Host homebrew-vm-mojave-2
	Port 22132

Host homebrew-vm-mojave-3
	Port 22133

